public boolean catDog(String str) {
  int c1=0;
  int c2=0;
  int n=str.length();
  for(int i=0;i<n-2;i++)
  {
    if(str.charAt(i)=='c' && str.charAt(i+1)=='a' && str.charAt(i+2)=='t')
    c1++;
    if(str.charAt(i)=='d' && str.charAt(i+1)=='o' && str.charAt(i+2)=='g')
    c2++;
    
  }
  if(c1==c2)
  return true;
  else 
  return false;
}
